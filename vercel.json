{
  "version": 2,
  "functions": {
    "api/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "ADMIN_USERNAME": "admin",
    "ADMIN_PASSWORD": "admin123",
    "JWT_SECRET": "viralvpn-super-secure-jwt-secret-key-2024"
  }
} 